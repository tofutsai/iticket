@model IEnumerable<prjITicket.Models.Activity>

@{
    ViewBag.Title = "商家中心";
    Layout = "~/Views/Shared/_BackSellerLayout.cshtml";
    int count = 0;

}
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<div class="form-group sticky-top" style="z-index:1500">
    <div class="col-md-offset-0 col-md-10 ">
        <input type="text" id="txtQuery" name="txtQuery" style="margin-top:10px;margin-left:-10px;" />
        <input id="Query" type="button" value="查詢" class="btn btn-secondary" />
    </div>
</div>
<div id="QResult">

</div>

<script>
    $(function () {
        getPage();
    $("#Query").click(function () {
        getPage();
    });
});
    function getPage() {
        $.ajax({
                    type: "get",
                    url: `@Url.Action("GetQueryResult", "SellerCenter")?txtQuery=${$("#txtQuery").val()}`,
                    success: function (result) {
                        $("#QResult").html(result);
                    }
        });
    }
